{
  "name": "pull-lock",
  "version": "0.0.1",
  "description": "",
  "main": "build/index.js",
  "author": "xtina-starr <c.thompsonms@gmail.com> & Art.sy Inc",
  "license": "MIT",
  "bin": {
    "pull-lock": "./build/index.js"
  },
  "devDependencies": {
    "@types/jest": "^19.2.3",
    "@types/node": "^10.11.2",
    "danger": "^0.18.0",
    "husky": "^0.14.3",
    "jest": "^20.0.0",
    "jest-cli": "^22.2.1",
    "lint-staged": "^3.2.5",
    "ts-jest": "^20.0.0",
    "ts-node": "^3.0.0",
    "tslint": "^5.2.0",
    "typescript": "^2.3.2"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "precommit": "lint-staged",
    "prepush": "npm run build",
    "postmerge": "pull-lock"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "tslint --fix",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(.test)\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
